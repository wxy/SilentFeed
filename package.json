{
  "name": "silentfeed",
  "displayName": "__MSG_extName__",
  "version": "0.2.0",
  "description": "__MSG_extDescription__",
  "author": "xingyu.wang@gmail.com",
  "scripts": {
    "dev": "bash scripts/watch-dnr.sh & plasmo dev",
    "build": "plasmo build && bash scripts/copy-locales.sh && bash scripts/setup-dnr.sh",
    "package": "plasmo package",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "i18n:extract": "i18next-parser",
    "i18n:translate": "node scripts/translate.js",
    "i18n:sync": "npm run i18n:extract && npm run i18n:translate",
    "pre-push": "bash scripts/pre-push-check.sh"
  },
  "dependencies": {
    "dexie": "^4.2.1",
    "fast-xml-parser": "^5.3.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "natural": "^8.1.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^16.2.3",
    "rss-parser": "^3.13.0",
    "stopword": "^3.1.4",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.0.258",
    "@types/jsdom": "^27.0.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/stopword": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "fake-indexeddb": "^6.2.4",
    "happy-dom": "^20.0.10",
    "i18next-parser": "^9.3.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.15",
    "typescript": "5.3.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  },
  "manifest": {
    "default_locale": "en",
    "host_permissions": [
      "https://*/*",
      "http://localhost:11434/*",
      "http://127.0.0.1:11434/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "alarms",
      "notifications",
      "declarativeNetRequestWithHostAccess"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/icons/128/*.png",
          "assets/icons/128/overlays/*.png"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
